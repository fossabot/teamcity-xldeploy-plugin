<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">
  <bean name="XldDeployRunner" class="jetbrains.buildServer.xldeploy.agent.XldDeployBuildServiceFactory" />
  <bean name="XldDeployAgentListener" class="jetbrains.buildServer.xldeploy.agent.XldDeployAgentListener" />
  <bean name="XldPublishRunner" class="jetbrains.buildServer.xldeploy.agent.XldPublishBuildServiceFactory" />
  <bean name="XldPublishAgentListener" class="jetbrains.buildServer.xldeploy.agent.XldPublishAgentListener" />
</beans>

